<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>EventEaseAppBlazor</RootNamespace>
    <AssemblyName>EventEaseAppBlazor</AssemblyName>
    <OutputType>Exe</OutputType>
    <Nullable>enable</Nullable>
    <UseRazorBuildServer>true</UseRazorBuildServer>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="6.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="6.0.0" />
  </ItemGroup>

</Project>